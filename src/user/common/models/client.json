{
  "name": "Client",
  "plural": "Clients",
  "base": "User",
  "idInjection": false,
  "forceId": false,
  "strict": true,
  "options": {
    "validateUpsert": true,
    "mongodb": {
      "collection": "client"
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "lastname": {
      "type": "string",
      "required": true
    },
    "botName": {
      "type": "string",
      "required": true
    },
    "botEmail": {
      "type": "string",
      "required": true
    },
    "creationDate": {
      "type": "date",
      "required": false,
      "defaultFn": "now"
    },
    "timeStart": {
      "type": "string",
      "required": false,
      "default": "9"
    },
    "timeEnd": {
      "type": "string",
      "required": false,
      "default": "18"
    },
    "blacklistedContacts": {
      "type": "array",
      "required": false,
      "default": []
    },
    "realm": false,
    "username": false
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
