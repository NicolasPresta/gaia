{
  "name": "Client",
  "plural": "Clients",
  "base": "User",
  "idInjection": false,
  "forceId": false,
  "strict": true,
  "options": {
    "validateUpsert": true,
    "mongodb": {
      "collection": "client"
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "lastname": {
      "type": "string",
      "required": true
    },
    "botName": {
      "type": "string",
      "required": true
    },
    "botEmail": {
      "type": "string",
      "required": true
    },
    "googleAccessToken": {
      "type": "string",
      "required": false
    },
    "googleRefreshToken": {
      "type": "string",
      "required": false
    },
    "creationDate": {
      "type": "date",
      "required": false,
      "defaultFn": "now"
    },
    "realm": false,
    "username": false
  },
  "validations": [],
  "relations": {},
  "acls": [
	{
		"principalType": "ROLE",
		"principalId": "$everyone",
		"permission": "ALLOW"
	}
  ],
  "methods": {}
}
